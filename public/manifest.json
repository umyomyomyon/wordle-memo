{
  "manifest_version": 2,
  "name": "Wordle Memo",
  "description": "Memo for Wordle.",
  "version": "1.0.0",
  "permissions": ["storage"],
  "browser_action": {
    "default_icon": {
      "16": "wordle-memo-icon16.png",
      "48": "wordle-memo-icon48.png",
      "128": "wordle-memo-icon128.png"
    },
    "default_popup": "index.html"
  },
  "icons": {
    "16": "wordle-memo-icon16.png",
    "48": "wordle-memo-icon48.png",
    "128": "wordle-memo-icon128.png"
  }
}
